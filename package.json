    "@reduxjs/toolkit": "1.7.2",
    "@rtk-incubator/action-listener-middleware": "0.8.0",
    "@rtk-query/graphql-request-base-query": "1.0.3",
    "framer-motion": "6.2.8",
    "graphql": "16.3.0",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6"
  },
