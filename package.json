{
  "name": "advertisy",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "graphql-codegen && vite --mode development",
    "start-staging": "graphql-codegen && vite --mode staging",
    "start-prod": "graphql-codegen && vite --mode production",
    "build": "graphql-codegen && vite --mode production build",
